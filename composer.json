{
  "name": "mrjgreen/db-sync",
  "type": "library",
  "description": "Efficient MySQL comparison and sync utility",
  "license" : "MIT",
  "autoload": {
    "psr-4": {
      "DbSync\\": "src"
    }
  },
  "bin": [
    "bin/sync"
  ],
  "scripts" : {
    "test" : "php vendor/bin/phpunit",
    "package" : "composer install --no-dev && ./phar-composer.phar build"
  },
  "require": {
    "psr/log": "~1.0",
    "symfony/console": ">=3.2",
    "mrjgreen/database": "dev-patch-1",
    "symfony/debug": "~3.2",
    "ramsey/array_column": "~1.1"
  },
  "require-dev": {
    "satooshi/php-coveralls": "~1.0",
    "phpunit/phpunit": "~5.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/vjccruz/database"
    }
  ]
}
